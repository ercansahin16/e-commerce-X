@model IEnumerable<Supplier>
@* markalar ve Footer bolümü *@
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.3/font/bootstrap-icons.css">

<div style="margin-top:155px"></div>
<style>

</style>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css" integrity="sha512-Fo3rlrZj/k7ujTnHgSpu6k4UHH5n2+R7K4t5pPpXvF4g8NKkn8o8L60ZfZJxQUHl7e2FJ1q4OCuAfAcs1lyyQ==" crossorigin="anonymous" referrerpolicy="no-referrer" />
<!-- Brand area start -->
<div class="brand-area">
	<div class="container">
		<div class="row">
			<div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
				<div class="all-p-title">
					<h3 style=""></h3>
				</div>
			</div>
			<div class="clear"></div>
			<div class="all-brand singal-p">
				@foreach (var item in Model)
				{
					<div class="singal_brand">
						<div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
							<a href="/Home/SupplierPage/@item.SupplierID"> 
								<img src="~/resimler/@item.PhotoPath" alt="" style="width:130px;height:50px;border-radius:6px"/> 
								</a>
						</div>
					</div>
				}
			</div>
		</div>
	</div>
</div>
<!-- footer top area start -->
<!-- Remove the container if you want to extend the Footer to full width. -->
<div class="container my-5">

   <footer class="text-white text-center text-lg-start bg-dark">
      <!-- Grid container -->
      <div class="container p-4">
         <!--Grid row-->
         <div class="row mt-4">
            <!--Grid column-->
            <div class="col-lg-4 col-md-12 mb-4 mb-md-0">
               <h5 class="text-uppercase mb-4">Hakkımızda</h5>

               <p>
                  Biz, müşteri memnuniyetini her zaman ön planda tutan bir e-ticaret platformuyuz.
               </p>

               <p>
                  Sektördeki deneyimimizle, kaliteli ve çeşitli ürünleri sizlerle buluşturuyoruz. Alışveriş deneyiminizi kolay ve keyifli hale getirmek için buradayız.
               </p>

               <div class="mt-4">
                  <!-- Facebook -->
                  <a type="button" class="btn btn-floating btn-light btn-lg"><i class="fab fa-facebook-f"></i></a>
                  <!-- Dribbble -->
                  <a type="button" class="btn btn-floating btn-light btn-lg"><i class="fab fa-github"></i></a>
                  <!-- Twitter -->
                  <a type="button" class="btn btn-floating btn-light btn-lg"><i class="fab fa-twitter"></i></a>
                  <!-- Google + -->
                  <a type="button" class="btn btn-floating btn-light btn-lg"><i class="fab fa-google-plus-g"></i></a>
                  <!-- Linkedin -->
               </div>
            </div>
            <!--Grid column-->
            <!--Grid column-->
            <div class="col-lg-4 col-md-6 mb-4 mb-md-0">
               <h5 class="text-uppercase mb-4 pb-1">Ürün ara</h5>

               <div class="form-outline form-white mb-4">
                  <input type="text" id="formControlLg" class="form-control form-control-lg" />
                  <label class="form-label" for="formControlLg"></label>
               </div>

               <ul class="fa-ul" style="margin-left: 1.65em;">
                  <li class="mb-3">
                     <span class="fa-li"><i class="fas fa-home"></i></span><span class="ms-2">İstanbul / Maltepe</span>
                  </li>
                  <li class="mb-3">
                     <span class="fa-li"><i class="fas fa-envelope"></i></span><span class="ms-2">esahin.ast@gmail.com</span>
                  </li>
                  <li class="mb-3">
                     <span class="fa-li"><i class="fas fa-phone"></i></span><span class="ms-2">+90 534 013 67 73</span>
                  </li>
               </ul>
            </div>
            <!--Grid column-->
            <!--Grid column-->
           
          
            <div class="col-lg-4 col-md-6 mb-4 mb-md-0">
               <div class="p-0 mt-2 colorbrown">
                  <b>E-BÜLTEN</b>
               </div>
               <div class="mb-3 colorgray pt-1">
                  Yeni ürünleri, indirimleri ve haberleri ilk sen duymak istiyorsan e-bültene kaydol.
               </div>
                <form action=@* " https://www.vienurla.com/subscribe1 " *@ method="POST" >
                  <div class="mb-4 colorgray">
                     <input type="checkbox" class="mr-2" name="kvkk" value="accepted" required="" data-gtm-form-interact-field-id="0"><b><span style="color:#4D6172"><a href=@* "https://www.vienurla.com/kurumsal/form-ve-sozlesmeler/kisisel-verilerin-korunmasi11/3" *@>KVKK</a></span>'nu okudum kabul ediyorum.</b>
                  </div>
                  <input type="hidden" >                        <div class="form-box p-1">
                     <input type="email" id="email" name="email" value="" placeholder="esahin.ast@gmail.com" class="form-control" autocomplete="on" required="">
                     <button type="submit" class="btn " style="margin-top:15px;background-color:grey;float:right">Gönder</button>
                  </div>
               </form>
            </div>
            <!--Grid column-->
         </div>
         <!--Grid row-->
      </div>
      <!-- Grid container -->
      <!-- Copyright -->
      <div class="text-center p-3" style="background-color: rgba(0, 0, 0, 0.2);">
         © 2024 Copyright:
         <a class="text-white" href="https://www.linkedin.com/in/ercansahin16/">trendcards</a>
      </div>
      <!-- Copyright -->
   </footer>


</div>
<!-- End of .container -->
<!-- footer top area end -->